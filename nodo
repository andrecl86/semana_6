public class Nodo
{
    public int Valor { get; set; }       // Dato almacenado en el nodo
    public Nodo Siguiente { get; set; }  // Referencia al siguiente nodo

    // Constructor
    public Nodo(int valor)
    {
        Valor = valor;
        Siguiente = null;  // Inicialmente no apunta a ning√∫n nodo
    }
}
